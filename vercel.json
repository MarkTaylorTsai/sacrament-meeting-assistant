{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "public",
  "functions": {
    "api/webhook.ts": {
      "maxDuration": 30
    },
    "api/reminder.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "/api/webhook"
    },
    {
      "source": "/reminder",
      "destination": "/api/reminder"
    }
  ]
}
